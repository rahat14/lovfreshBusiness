<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorAccent</item>
        <item name="colorPrimaryDark">@color/colorPrimary</item>
        <item name="colorAccent">@color/colorAccent</item>
        <item name="android:windowTranslucentStatus">true</item>
        <item name="android:windowTranslucentNavigation">true</item>
    </style>

    <style name="AppTheme.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="AppTheme.PopupOverlay" parent="ThemeOverlay.AppCompat.Light"></style>

    <style name="AlertDialogCustom" parent="ThemeOverlay.AppCompat.Dialog.Alert">
        <item name="android:textColor">@color/colorPrimary</item>
    </style>

    <style name="testHeadingStyle">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">24sp</item>
        <item name="android:gravity">center</item>
        <item name="android:textColor">@color/colorAccent</item>
        <item name="android:fontFamily">@font/poppins_semibold</item>
        <item name="android:ellipsize">end</item>
    </style>

    <style name="testHeadingNormalStyle">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">20sp</item>
        <item name="android:gravity">center</item>
        <item name="android:textColor">@color/text_header_color</item>
        <item name="android:fontFamily">@font/poppins_regular</item>
        <item name="android:ellipsize">end</item>
    </style>

    <style name="testStyle">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">15sp</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:ellipsize">end</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:gravity">center</item>
        <item name="android:maxLines">1</item>
        <item name="android:fontFamily">@font/poppins_regular</item>
    </style>

    <style name="testsecondStyle">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">15sp</item>
        <item name="android:textColor">@color/fill_text</item>
        <item name="android:ellipsize">end</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:fontFamily">@font/poppins_regular</item>
    </style>

    <style name="viweLineStyle">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">10dp</item>
        <item name="android:background">@color/colorAccent</item>
    </style>

    <style name="viewLineStyle">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">0.5dp</item>
        <item name="android:background">@color/stroke_color</item>
    </style>

    <style name="imageStyle">
        <item name="android:layout_width">26dp</item>
        <item name="android:layout_height">26dp</item>
        <item name="android:gravity">center</item>
    </style>

    <style name="imageStyle_2">
        <item name="android:layout_width">30dp</item>
        <item name="android:layout_height">30dp</item>
        <item name="android:gravity">center</item>
    </style>

    <style name="imageStyle2">
        <item name="android:layout_width">24dp</item>
        <item name="android:layout_height">24dp</item>
        <item name="android:gravity">center</item>
    </style>

    <style name="back_imageStyle">
        <item name="android:layout_width">40dp</item>
        <item name="android:layout_height">40dp</item>
        <item name="android:gravity">center</item>
    </style>

    <style name="testMediumStyle">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">12sp</item>
        <item name="android:textColor">@color/teal_200</item>
        <item name="android:ellipsize">end</item>
        <item name="android:layout_marginTop">15dp</item>
        <item name="android:fontFamily">@font/poppins_medium</item>
    </style>

    <style name="testMediumSecondStyle">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">15sp</item>
        <item name="android:textColor">@color/text_color</item>
        <item name="android:ellipsize">end</item>
        <item name="android:fontFamily">@font/poppins_medium</item>
    </style>


    <style name="testMediumThirdStyle">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">18sp</item>
        <item name="android:textColor">@color/colortextAccent</item>
        <item name="android:ellipsize">end</item>
        <item name="android:fontFamily">@font/poppins_semibold</item>
    </style>


    <style name="testMediumFivthStyle">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:gravity">left</item>
        <item name="android:layout_gravity">center_vertical</item>
        <item name="android:textSize">15sp</item>
        <item name="android:textColor">@color/payment_hint_color</item>
        <item name="android:ellipsize">end</item>
        <item name="android:fontFamily">@font/poppins_regular</item>
        <item name="android:background">@drawable/bg_white_box_with_gray</item>
    </style>


    <style name="testMediumTitleStyle">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">18sp</item>
        <item name="android:textColor">@color/fill_text</item>
        <item name="android:fontFamily">@font/poppins_semibold</item>
    </style>

    <style name="testMediumFourthStyle">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">15sp</item>
        <item name="android:textColor">@color/text_color</item>
        <item name="android:ellipsize">end</item>
        <item name="android:layout_marginTop">15dp</item>
        <item name="android:fontFamily">@font/poppins_medium</item>
    </style>

    <style name="editTextstyle">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">45dp</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:textColorHint">@color/text_color</item>
        <item name="android:textSize">14sp</item>
        <item name="android:paddingLeft">15dp</item>
        <item name="android:paddingRight">5dp</item>
        <item name="background">@drawable/bg_white_box_with_gray</item>
        <item name="android:fontFamily">@font/poppins_regular</item>
    </style>

    <style name="mytheme" parent="Theme.AppCompat.Light.Dialog">
        <item name="windowNoTitle">true</item>
    </style>


    <style name="editTextThirdtyle">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:textColorHint">@color/white</item>
        <item name="android:textSize">13sp</item>
        <item name="android:paddingLeft">15dp</item>
        <item name="android:paddingRight">5dp</item>
        <item name="android:fontFamily">@font/poppins_regular</item>
        <item name="android:maxLines">1</item>
    </style>

    <style name="editTexttyle_textInput">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:textColorHint">@color/black</item>
        <item name="android:textSize">14sp</item>
        <item name="android:paddingLeft">15dp</item>
        <item name="android:paddingRight">5dp</item>
        <item name="android:fontFamily">@font/poppins_regular</item>
        <item name="android:maxLines">1</item>
    </style>

    <style name="imageStyle_3">
        <item name="android:layout_width">25dp</item>
        <item name="android:layout_height">25dp</item>
        <item name="android:gravity">center</item>
    </style>
    <style name="imageStyle_4">
        <item name="android:layout_width">28dp</item>
        <item name="android:layout_height">28dp</item>
        <item name="android:gravity">center</item>
    </style>

    <style name="no_data_imageStyle">
        <item name="android:layout_width">150dp</item>
        <item name="android:layout_height">150dp</item>
        <item name="android:gravity">center</item>
    </style>

    <style name="TextLabel" parent="TextAppearance.Design.Hint">
        <item name="android:textSize">13sp</item>
        <item name="colorControlNormal">@color/black</item>
        <item name="android:fontFamily">@font/poppins_regular</item>

    </style>

    <style name="buttonStyle">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">50dp</item>
        <item name="android:maxLines">1</item>
        <item name="android:textSize">18sp</item>
        <item name="android:layout_marginLeft">10dp</item>
        <item name="android:layout_marginRight">10dp</item>
        <item name="android:layout_gravity">center</item>
        <item name="android:gravity">center</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:fontFamily">@font/poppins_medium</item>
        <item name="android:background">@drawable/bg_button</item>
    </style>

    <style name="backStyle">
        <item name="android:layout_width">24sp</item>
        <item name="android:layout_height">24sp</item>
        <item name="android:src">@drawable/ic_back_arrow</item>
    </style>

    <style name="TextLabel_1" parent="Widget.MaterialComponents.TextInputLayout.OutlinedBox">
        <item name="android:textColorHint">@color/black</item>
        <item name="android:textSize">13sp</item>
        <item name="colorControlNormal">@color/stroke_color</item>
        <item name="colorControlActivated">@color/colorPrimary</item>
        <item name="colorControlHighlight">@color/colorPrimary</item>
        <item name="android:fontFamily">@font/poppins_regular</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">55dp</item>
    </style>

    <style name="buttonStyle2">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">50dp</item>
        <item name="android:maxLines">1</item>
        <item name="android:textSize">14sp</item>
        <item name="android:layout_marginLeft">10dp</item>
        <item name="android:layout_marginRight">10dp</item>
        <item name="android:layout_gravity">center</item>
        <item name="android:textColor">#B7B7B7</item>
        <item name="android:fontFamily">@font/poppins_medium</item>
        <item name="android:background">@drawable/bg_wihte_button</item>
    </style>

    <style name="popupstyle" parent="android:Theme.Holo.Light">
        <item name="textAppearanceSmallPopupMenu">@style/myPopupMenuTextAppearanceColor</item>
        <item name="android:popupMenuStyle">@style/myPopupMenuStyle</item>
    </style>

    <style name="myPopupMenuStyle" parent="@style/Widget.AppCompat.Light.PopupMenu">

    </style>

    <style name="myPopupMenuTextAppearanceColor" parent="@style/TextAppearance.AppCompat.Light.Widget.PopupMenu.Small">
        <item name="android:textColor">@color/colorAccent</item>
    </style>

    <style name="MyCustomTextAppearance" parent="TextAppearance.Design.Tab">
        <item name="textAllCaps">false</item>
        <item name="android:textSize">15sp</item>
        <item name="fontFamily">@font/poppins_semibold</item>
    </style>
</resources>
